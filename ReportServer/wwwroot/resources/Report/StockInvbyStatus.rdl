  <DataSets>
    <DataSet Name="StockIn">
      <Query>
        <DataSourceName>inventory</DataSourceName>
        <CommandText>select barcode, s.description, 
--l.description location, 
--sec.description section,
sum(quantity) quantity ,s.MinQty, s.MaxQty, ss.Description Status
From stocktx st
join stockmaster s on s.id = st.stockid and s.deleted2 = 0
--join location l on l.id = st.location and l.deleted2 = 0
--join section sec on sec.id = st.section and sec.deleted2 = 0
join StockStatus ss on ss.id = s.Status and ss.Deleted2 = 0
where st.deleted2 = 0 
--and cast(st.createdate as date) &gt;= @fromdate
--and cast(st.createdate as date) &lt;= @todate
--and l.id in ( @location )
--and sec.id in ( @section )
group by barcode, s.description, 
--l.description, 
--sec.description,
s.MinQty, s.MaxQty, ss.Description
order by 
--sec.description, 
--l.description, 
 s.description, barcode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
