  <DataSets>
    <DataSet Name="StockAdj">
      <Query>
        <DataSourceName>inventory</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@frmDate">
            <Value>=Parameters!frmdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@todate">
            <Value>=Parameters!todate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@section">
            <Value>=Parameters!section.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@location">
            <Value>=Parameters!location.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stock">
            <Value>=Parameters!stock.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select TxDate, Barcode , sm.Description, s.Description section , l.Description location, sum(bsh.Qty) Qty
from BatchStockHold bsh
join InventoryBatch ib on ib.Deleted2 = 0 and ib.id = bsh.BatchID
join Location l on l.id = ib.LocationID and l.Deleted2 = 0
join Section s on s.id = ib.SectionID  and s.Deleted2 = 0
join StockMaster sm on sm.id = ib.StockID and sm.Deleted2 = 0
where bsh.Deleted2 = 0
and cast(TxDate as date) &gt;= case when @frmDate = '' or @frmDate is null then '1900-01-01' else @frmDate end
and cast(TxDate as date) &lt;= case when @todate = '' or @todate is null then '2030-12-31' else @todate end
and s.id in ( @section)
and l.id in ( @location )
and sm.id in ( @stock )
group by TXDate, Barcode , sm.Description, s.Description  , l.Description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
