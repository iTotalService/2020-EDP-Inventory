  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EDP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Stock">
            <Value>=Parameters!Stock.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@section">
            <Value>=Parameters!section.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@location">
            <Value>=Parameters!location.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TXStatus">
            <Value>=Parameters!TXStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select Barcode, sm.Description, MinQty, MaxQty
,tx.section,tx.location, txStatus, tx.Remarks,
Qty,
displayQty
--,TXDate
from StockMaster sm
join (
select StockID, l.Description [location], s.Description section , remarks, 
Case when remarks = 'Stock Out' then 'Consumed' else 'Available' end txStatus,
sum( Quantity ) Qty,
sum( case when Quantity &lt; 0 then -1 else 1 end  * Quantity ) displayQty
--, cast(tx.createdate as date) TXDate
from 
StockTX tx 
join [Location] l on l.id = tx.Location and l.Deleted2 = 0
join Section s on s.id = tx.Section and s.Deleted2 = 0
where tx.Deleted2 = 0
and tx.remarks in ( @TXStatus )
group by StockID, l.Description , s.Description  ,remarks
--, tx.createdate
union
select ib.StockID, l.Description [location], s.Description section  ,'', ss.Description
,sum(bsh.Qty * -1)
,sum(bsh.Qty )
from InventoryBatch ib 
join BatchStockHold bsh on ib.ID = bsh.BatchID and bsh.Deleted2 = 0
join [Location] l on l.id = ib.Locationid and l.Deleted2 = 0
join Section s on s.id = ib.Sectionid and s.Deleted2 = 0
join StockStatus ss on ss.id = bsh.stockstatus
and ib.Deleted2 = 0
group by ib.StockID, l.Description, s.Description, ss.Description
) tx on sm.ID = tx.StockID
where sm.Deleted2 = 0
and sm.ID in ( @Stock )
and tx.section in ( @section )
and tx.Location in ( @location )
and sm.Status in ( @Status )</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
