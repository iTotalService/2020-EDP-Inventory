                            <div id='sb-he-text' class='e-sb-header-text'>
                                <span class='sb-header-text-left'>@*@User.Claims.FirstOrDefault(m => m.Type == "Account").Value*@</span>
                            </div>
                            <div class='sb-header-item sb-table-cell sb-lang-toggler-wrapper'>
                                <span id='sb-switcher' role="button" tabindex="0" class='sb-lang-toggler sb-icons sb-icon-Dropdown' aria-label="User Menu"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class='sb-control-navigation'>
